cur.initEdit = function(cont, plain) {
  cont = 'dev_edit_'+cont;
  var objCont = ge(cont);
  if (!objCont) {
    return;
  }
  var editor = new WkEditor(objCont, {
    buttons: {
      bold: [0, 0, 'Bold'],
      italic: [0, -20, 'Italic'],
      left: [0, -80, 'Align left'],
      center: [0, -100, 'Align center'],
      right: [0, -120, 'Align right'],
      list: [0, -140, 'Bulleted list', 'List item'],
      h1: [0, -180, 'Add heading', 'Enter heading text'],
      h2: [0, -200, 'Add subheading', 'Enter heading text'],
      h3: [0, -220, 'Add subheading second level', 'Enter heading text'],
      blockquote: [0, -360, 'Add quote', 'Quoted text'],
      url: [0, -300, 'Add link', 'Delete link'],
      image: [0, -240, 'Add photo'],
      video: [0, -260, 'Add video'],
      audio: [0, -280, 'Add audio']
    },
    plain: plain,
    wikiPref: '',
    source: ge(cont+'_textarea'),
    panelCont: ge(cont+'_buttons_cont'),
    help: ['Wiki markup help'],
    mode: [0, -340, 'Wiki markup mode', 'Visual editing mode'],
    lang: {
      too_long: 'This page is too long to be saved.'
    },
    onPlainToggle: function(on, btn) {
      if (btn) {
        ajax.post('al_pages.php', {act: 'toggle_plain', state: on ? 1 : 0}, {})
      }
      if (on) {
        show('wk_wiki_help');
      } else {
        hide('wk_wiki_help');
      }
    }
  });
  cur.editors.push(editor);
}

if (ge('dev_section_menu')) {
  cur.methodsDD = new DropdownMenu(sections, {
    target: ge('dev_section_menu'),
    width: 190,
    showHover: false,
    offsetTop: -3,
    containerClass: 'dev_section_dd',
    updateTarget: false,
    align: 'right',
    onSelect: function(obj) {
      var sect = cur.methodsDD.val();
      Dev.switchSection(sect, true);
    },
    onShow: function() {
      var rows = cur.methodsDD.rows;
      if (rows) {
        var rowsWrap = domPN(rows), rowsH = getSize(rows)[1],
            rowsWrapH = Math.min(rowsH, Math.max(200, window.lastWindowHeight + scrollGetY() - getXY(rows)[1] - 2));
        setStyle(rowsWrap, {height: rowsWrapH, overflowY: (rowsWrapH < rowsH ? 'scroll' : 'auto')});
        rows.scrollTop = 0;
      }
      if (!isVisible('box_layer_wrap')) {
        addClass(bodyNode, 'hat_on_top');
      }
    },
    onHide: function() {
      removeClass(bodyNode, 'hat_on_top');
    }
  });
